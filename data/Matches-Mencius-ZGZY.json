    }
  ],
  "nodes": [
    {
      "group": 0,
      "l": 7,
      "index": 625,
      "id": "A625",
      "text": "者未之有也狗彘"
    },
    {
      "group": 3,
      "l": 7,
      "index": 27598,
      "id": "B27598",
      "text": "者未之有也可即"
    },
    {
      "group": 0,
      "l": 7,
      "index": 2442,
      "id": "A2442",
      "text": "者未之有也梁惠"
    },
    {
      "group": 0,
      "l": 7,
      "index": 3304,
      "id": "A3304",
      "text": "者未之有也昔者"
    },
    {
      "group": 0,
      "l": 7,
      "index": 7397,
      "id": "A7397",
      "text": "者未之有也孟子"
    },
    {
      "group": 0,
      "l": 7,
      "index": 15689,
      "id": "A15689",
      "text": "者未之有也聖人"
    },
    {
      "group": 0,
      "l": 7,
      "index": 16963,
      "id": "A16963",
      "text": "者未之有也不誠"
    },
    {
      "group": 0,
      "l": 7,
      "index": 18578,
      "id": "A18578",
      "text": "者未之有也孟子"
    },
    {
      "group": 0,
      "l": 7,
      "index": 28903,
      "id": "A28903",
      "text": "者未之有也先生"
    },
    {
      "group": 0,
      "l": 7,
      "index": 28998,
      "id": "A28998",
      "text": "者未之有也何必"
    },
    {
      "group": 0,
      "l": 7,
      "index": 33649,
      "id": "A33649",
      "text": "者未之有也孟子"
    },
    {
      "group": 0,
      "l": 7,
      "index": 3238,
      "id": "A3238",
      "text": "子對曰有人不得"
    },
    {
      "group": 3,
      "l": 7,
      "index": 56166,
      "id": "B56166",
      "text": "子對曰有難不死"
    },
    {
      "group": 0,
      "l": 11,
      "index": 3311,
      "id": "A3311",
      "text": "齊景公問於晏子曰吾欲觀"
    },
    {
      "group": 3,
      "l": 11,
      "index": 56149,
      "id": "B56149",
      "text": "齊景公問於晏子曰忠臣之"
    },
    {
      "group": 0,
      "l": 7,
      "index": 5506,
      "id": "A5506",
      "text": "之所不為也而子"
    },
    {
      "group": 3,
      "l": 7,
      "index": 43387,
      "id": "B43387",
      "text": "之所不為也自王"
    },
    {
      "group": 0,
      "l": 7,
      "index": 10905,
      "id": "A10905",
      "text": "也孔子曰君薨聽"
    },
    {
      "group": 3,
      "l": 7,
      "index": 75494,
      "id": "B75494",
      "text": "也孔子曰君使臣"
    },
    {
      "group": 0,
      "l": 9,
      "index": 9317,
      "id": "A9317",
      "text": "自天子達於庶人非直"
    },
    {
      "group": 3,
      "l": 9,
      "index": 49016,
      "id": "B49016",
      "text": "自天子達於庶人也又"
    },
    {
      "group": 0,
      "l": 9,
      "index": 10774,
      "id": "A10774",
      "text": "自天子達於庶人三代"
    },
    {
      "group": 0,
      "l": 7,
      "index": 13067,
      "id": "A13067",
      "text": "曰昔齊景公田招"
    },
    {
      "group": 3,
      "l": 7,
      "index": 12745,
      "id": "B12745",
      "text": "曰昔齊景公以馬"
    },
    {
      "group": 3,
      "l": 7,
      "index": 72512,
      "id": "B72512",
      "text": "曰昔齊景公時彗"
    },
    {
      "group": 0,
      "l": 7,
      "index": 13446,
      "id": "A13446",
      "text": "曰古之君子仕乎"
    },
    {
      "group": 3,
      "l": 7,
      "index": 56093,
      "id": "B56093",
      "text": "曰古之君子進人"
    },
    {
      "group": 0,
      "l": 7,
      "index": 30452,
      "id": "A30452",
      "text": "曰古之君子何如"
    },
    {
      "group": 0,
      "l": 11,
      "index": 15674,
      "id": "A15674",
      "text": "不愆不忘率由舊章遵先王"
    },
    {
      "group": 3,
      "l": 11,
      "index": 27576,
      "id": "B27576",
      "text": "不愆不忘率由舊章夫不學"
    },
    {
      "group": 0,
      "l": 12,
      "index": 16052,
      "id": "A16052",
      "text": "殷鑒不遠在夏后之世此之謂"
    },
    {
      "group": 3,
      "l": 12,
      "index": 61753,
      "id": "B61753",
      "text": "殷鑒不遠在夏后之世又曰伐"
    },
    {
      "group": 0,
      "l": 14,
      "index": 18134,
      "id": "A18134",
      "text": "視臣如手足則臣視君如腹心君之"
    },
    {
      "group": 3,
      "l": 14,
      "index": 56350,
      "id": "B56350",
      "text": "視臣如手足臣視君如腹心君視臣"
    },
    {
      "group": 0,
      "l": 13,
      "index": 18154,
      "id": "A18154",
      "text": "臣視君如國人君之視臣如土芥"
    },
    {
      "group": 3,
      "l": 13,
      "index": 56367,
      "id": "B56367",
      "text": "臣視君如國人君視臣如糞土臣"
    },
    {
      "group": 0,
      "l": 7,
      "index": 18168,
      "id": "A18168",
      "text": "臣視君如寇讎王"
    },
    {
      "group": 3,
      "l": 7,
      "index": 56379,
      "id": "B56379",
      "text": "臣視君如寇仇雖"
    },
    {
      "group": 0,
      "l": 14,
      "index": 18148,
      "id": "A18148",
      "text": "視臣如犬馬則臣視君如國人君之"
    },
    {
      "group": 3,
      "l": 14,
      "index": 56362,
      "id": "B56362",
      "text": "視臣如犬馬臣視君如國人君視臣"
    },
    {
      "group": 0,
      "l": 7,
      "index": 18666,
      "id": "A18666",
      "text": "可立而待也故聲"
    },
    {
      "group": 3,
      "l": 7,
      "index": 63728,
      "id": "B63728",
      "text": "可立而待也24"
    }
  ],
